<div class="product__List">
    <section>
        <div class="container">
            <div class="flex_box">
                <div class="product_filter">
                    <input id="check01" type="checkbox" name="menu"/>
                    <label for="check01" class="btn"><i class="i-dropdown"></i>Fruits & Vegetable</label>
                    <ul class="dropdown-content">
                        {{#each cat_List}}
                        <li>
                            <a href="/product/{{this.id}}" >{{this.name}}</a>
                        </li>
                        {{/each}}
                   
                    </ul>
                </div>
                <div class="product_lists">
                    <ul>
                        {{#each prod_List}}
                        <li id="{{this.category}}">
                            <div>
                                <h4 class="match" tabindex="0">{{this.name}}</h4>
                                <div class="prod_description">
                                    <div class="prod_img"><img alt="fruits" src="{{this.imageURL}}"></div>
                                    <p tabindex="0">
                                       {{this.description}}
                                    </p>
                                    
                                <div class="buy_now">
                                    <a href="#" class="btn buy" role="button">Buy Now <span><span>MRP</span> Rs. {{this.price}}</span></a>
                                </div>
                                </div>
                            </div>
                        </li>
                        {{/each}}
                  
                    </ul>
                </div>
            </div>
        </div>
    </section>
</div>